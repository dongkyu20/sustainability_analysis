<script>
    $(".wrap-visual1 .slider").slick({
        slidesToShow: 1,
        slidesToScroll: 1,
        dots: true,
        arrows: true,
        prevArrow: $('button.prev'),
        nextArrow: $('button.next'),
        autoplay: true,
        autoplaySpeed: 3000,
        infinite: true,
        speed: 1000,
		pauseOnHover: false,
        
            
            
                fade: true,
            
        
        cssEase: 'cubic-bezier(0.7, 0, 0.3, 1)',
        appendDots: $('.wrap-visual1 .control .paging')
    });

    $('.wrap-visual1 button.play').on('click', function () {
        $(".wrap-visual1 .slider").slick("slickPlay");
        $(this).removeClass('on');
        $('.wrap-visual1 button.stop').addClass('on');
    });
    $('.wrap-visual1 button.stop').on('click', function () {
        $(".wrap-visual1 .slider").slick("slickPause");
        $(this).removeClass('on');
        $('.wrap-visual1 button.play').addClass('on');
    });
</script>






